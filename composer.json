{
    "name": "backstagetechnicalservices/website",
    "description": "The source code of the Backstage Technical Services website.",
    "keywords": [],
    "license": "GNU GPLv2",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "Eonasdan/bootstrap-datetimepicker": "^4.17",
        "components/jquery": "^3.1",
        "doctrine/dbal": "^2.5",
        "eluceo/ical": "^0.11.0",
        "fortawesome/font-awesome": "^4.7",
        "graham-campbell/markdown": "^7.1",
        "greggilbert/recaptcha": "^2.1",
        "guzzlehttp/guzzle": "^6.2",
        "intervention/image": "^2.3",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4",
        "moment/moment": "^2.17",
        "sammyk/laravel-facebook-sdk": "^3.4",
        "select2/select2": "^4.0",
        "szykra/laravel-flash-notifications": "^0.3.3",
        "twbs/bootstrap": "^3.3",
        "vespakoen/menu": "^3.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
